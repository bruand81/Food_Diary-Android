                    android:value="cloud.antares.diarioalimentare.MainActivity" />
        </activity>
        <activity android:name=".EditMealActivity" />
        <activity android:name=".EditEmotionActivity" />
        <activity
                android:name=".EmotionListActivity"
                android:label="@string/title_activity_emotion_list"
