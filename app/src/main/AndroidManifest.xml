    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name=".MeasureUnitEditActivity"></activity>
        <activity
                android:name=".MeasureUnitManagement"
                android:label="@string/title_activity_measure_unit_management"
                android:parentActivityName=".MainActivity"
                android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                    android:name="android.support.PARENT_ACTIVITY"
                    android:value="cloud.antares.diarioalimentare.MainActivity" />
        </activity>
        <activity
                android:name=".ExportActivity"
